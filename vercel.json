{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["prisma/schema.prisma", "node_modules/.prisma/**/*"] }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "api/index.js"
      }
    ]
  }